/ {
    compatible = "zmk,keyboard";
    
    chosen {
        zmk,kscan = &dummy_kscan;
    };

    dummy_kscan: kscan {
        compatible = "zmk,kscan_gpio";
        rows = <1>;
        columns = <1>;
        row-gpios = <>;
        col-gpios = <>;
    };

    zmk,usb {
        compatible = "zmk,usb";
        status = "okay";
    };

    bt_central: bt_central {
        compatible = "zmk,bluetooth";
        local-name = "LoPro Cosmos 3";
    };
};



