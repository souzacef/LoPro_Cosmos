/ {
    compatible = "zmk,keyboard";

    chosen {
        zmk,kscan = &kscan0;
    };

    kscan0: kscan {
        compatible = "zmk,kscan_dummy";
        rows = <1>;
        columns = <1>;
    };
    
    zmk,usb {
        compatible = "zmk,usb";
        status = "okay";
    };
    
    bt-central {
        compatible = "zmk,bluetooth";
        local-name = "LoPro Cosmos 3";
    };
};
